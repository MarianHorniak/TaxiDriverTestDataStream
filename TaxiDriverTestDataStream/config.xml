<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android"
        id          = "sk.datastream.taxidriverinternal"
        versionCode = "544"
        version     = "1.5.44">
 
    <name>TAXI Driver test datastream</name>
  
    <description>
        TAXI Driver test datastream application
    </description>
 
    <author href="http://www.datastream.sk" email="info@datastream.sk">
        DataStream
    </author>
  
  <icon src="img/phoneicon.png" />
  <icon src="img/phoneicon57.png"          gap:platform="ios" width="57"  height="57"  />
  <icon src="img/phoneicon72.png"       gap:platform="ios" width="72"  height="72"  />
  <icon src="img/phoneicon120.png"       gap:platform="ios" width="120"  height="120"  />
  <icon src="img/phoneicon152.png"       gap:platform="ios" width="152"  height="152"  />
  <icon src="img/phoneicon76.png"       gap:platform="ios" width="76"  height="76"  />

  <preference name="load-url-timeout" value="60000" />
  <preference name="phonegap-version" value="cli-7.1.0" />
  <preference name="pgb-builder-version" value="1" />
  <preference name="exit-on-suspend" value="false" />
  <preference name="KeepRunning " value="true" />
  <preference name="Fullscreen" value="true" />
  <preference name="loadingDialog" value="Driver, Starting" />
  <preference name="AndroidPersistentFileLocation" value="Compatibility" />
      
  <!--<preference name="android-minSdkVersion" value="17" />
  <preference name="android-targetSdkVersion" value="27" />-->

  <access origin="*" />

  <plugin name="cordova-plugin-media" spec="5.0.2" />
  <plugin name="cordova-plugin-geolocation" spec="4.0.1"/>
  <plugin name="cordova-plugin-inappbrowser" spec="*" />
  <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="4.2.2" />
  <plugin name="de.appplant.cordova.plugin.local-notification" spec="0.8.3" />
  
  
  <feature name="http://api.phonegap.com/1.0/geolocation"/>
  <feature name="http://api.phonegap.com/1.0/media"/>
  <feature name="http://api.phonegap.com/1.0/network"/>
  <feature name="http://api.phonegap.com/1.0/notification"/>
  <feature name="http://api.phonegap.com/1.0/device"/>
  
</widget>
